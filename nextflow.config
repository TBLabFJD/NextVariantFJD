
manifest {
	 description = 'FJD-pipeline for germline Variant Calling Analysis based on GATK best practices.'
	 nextflowVersion = '>= 21.10.6'
}

// cleanup = true


params {

	input     = []

	output    = []
	runname   = []
	//basespace = []

	analysis = []  // D (Download), M (Mapping), Q (quality), S (SNV individual), G (SNV GVCF), A (SNV annotation), C (CNV calling and annotation), X (chrX CNV calling)
	capture  = "P" // P (Pannel), E (Whole Exome), G (Whole Genome)

	errormissingsample = true
	samples  = []
	genelist = []
	glowgenes  = []

	bed       = []
	ped       = []

	intervals  = false
	padding    = 500
	
	window     = []
	min_target = 20
	cnv_chrx = false

	ignoreduplicates = [] // In case of amplicon-based library preparation it is better to ignore duplicates
	// qcnvthreshold    = 20 // Read depth that 90% ofregions must have to pass QC filter
	// qsnvthreshold    = 10 // Read depth that 90% ofregions must have to pass QC filter

	

	pathology = "healthy" // Disease group name: resp, digest, skin, musc, gu, pregnancy, perinatal, cong_mal, clinic, infectious, other, neoplasm, blood, endoc, mental, CNS, eye, ear, circ, healthy.	
	maf        = "0.1"
	
	// removebam  = []



	tasks = "/home/gonzalo/nextflowtest/NextVariantFJD/tasks"

	dbNSFP_gene = "/mnt/genetica7/vep_plugins_gene/dbNSFP4.3_gene.complete.pvm.txt"
	omim        = "/mnt/genetica7/vep_plugins_gene/omim_genemap2.txt"
	regiondict  = "/mnt/genetica7/vep_plugins_gene/dict_region.csv"
	loFtool     = "/mnt/genetica7/vep_plugins_gene/LoFtool_scores.txt"
	exACpLI     = "/mnt/genetica7/vep_plugins_gene/ExACpLI_values.txt"
	maxEntScan  = "/mnt/genetica7/vep_plugins_gene/maxEntScan"

	
	annotsv_path = "/home/gonzalo/software/AnnotSV/"
	
	baseuser  = []
	queue = "bioinfo" // bioinfo, fastbioinfo
	scratch = "/mnt/tblab/tmp/"

}



process {
	cpus = 2
	memory = 10.GB

	withLabel: mediumcpu { cpus = 4 } // HAPLOTYPECALLER, SELECT_SNV, SELECT_INDEL, SELECT_MIX, FILTRATION_SNV, FILTRATION_INDEL, FILTRATION_MIX, MERGE_VCF, GVCF_HAPLOTYPECALLER
	withLabel: mediummem { memory = 20.GB } // HAPLOTYPECALLER, SELECT_SNV, SELECT_INDEL, SELECT_MIX, FILTRATION_SNV, FILTRATION_INDEL, FILTRATION_MIX, MERGE_VCF, GVCF_HAPLOTYPECALLER
	
	withLabel: highcpu { cpus = 20 } // BWA, MARKDUPLICATESSPARK, VEP, DEEPVARIANT, ANNOTSV, MANTA, ANNOTSV_VCF
	withLabel: highmem { memory = 100.GB } // BWA, MARKDUPLICATESSPARK, VEP, DEEPVARIANT, ANNOTSV, MANTA, ANNOTSV_VCF, PVM, QC_SUMMARY

	// cpus   = { check_max( 1    * task.attempt, 'cpus'   ) }
	// memory = { check_max( 6.GB * task.attempt, 'memory' ) }
	// time   = { check_max( 4.h  * task.attempt, 'time'   ) }

	// errorStrategy = { task.exitStatus in [143,137,104,134,139] ? 'retry' : 'finish' }
	// maxRetries    = 1
	// maxErrors     = '-1'
	
}






// docker.enabled         = true
// docker.runOptions      = '-u $(id -u):$(id -g)'




profiles {
	batch {
		process.executor       = 'slurm'        
		process.clusterOptions = '--account=bioinfo_serv'
		process.clusterOptions = "--partition=${params.queue}"
	}



	docker {
		docker.enabled    = true
		docker.runOptions = '-u $(id -u):$(id -g)'
		process{
			withLabel: gatk { container = "broadinstitute/gatk:4.3.0.0" }
			withLabel: vep { container = "ensemblorg/ensembl-vep:release_105.0" }
			withLabel: deepvariant { container = "google/deepvariant:1.4.0" }
			withLabel: bioinfotools { container = "tblabfjd/bioinfotools:1.0.0"}
			withLabel: annotsv { container = "tblabfjd/annotsv:3.1.1"}
			withLabel: manta { container = "tblabfjd/manta:1.6.0"}
			withLabel: python { container = "tblabfjd/convading:1.2.1"}
			withLabel: bwa { container = "tblabfjd/bwa:0.7.17-r1198-dirty" }
			withLabel: basespace { 
				container = "tblabfjd/basespace:1.5.1" 
				containerOptions = '-e "HOME=${HOME}" -v $HOME:$HOME'
			}
		}

	}



	singularity {
		singularity.enabled = true
		singularity.autoMounts = true		
		process{
			withLabel: gatk { container = "/home/gonzalo/singularity_images/gatk_4.3.0.0.sif" }
			withLabel: vep { container = "/home/gonzalo/singularity_images/ensembl-vep_release_105.0.sif" }
